{
  "$schema": "https://dpmland.deno.dev/schema",
  "name": "dpm-web",
  "version": "0.1.0",
  "description": "The official DPM Website powered by Oak and Deno",
  "author": "The DPM Authors",
  "license": "MIT",
  "main": "mod.ts",
  "scripts": {
    "test": "deno test -A --unstable",
    "fmt": "deno fmt -c deno.json",
    "lint": "deno lint -c deno.json",
    "start": "deno run -A app.ts",
    "fmt:check": "deno fmt -c deno.json --check && deno lint -c deno.json"
  },
  "dependencies": {
    "datetime/": "0.152.0",
    "fmt/": "0.152.0",
    "path/": "0.152.0",
    "oak/": "v10.6.0",
    "view_engine/": "v10.6.0",
    "http_errors/": "3.0.0",
    "oak_logger/": "1.0.0"
  }
}
